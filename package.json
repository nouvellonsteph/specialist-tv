{
	"name": "specialist-tv",
	"version": "1.0.0",
	"description": "A comprehensive video knowledge management platform built for teams using the Cloudflare stack. Features AI-powered processing, intelligent search, and content discovery.",
	"keywords": [
		"video",
		"knowledge-management",
		"cloudflare",
		"ai",
		"transcription",
		"search",
		"nextjs"
	],
	"author": "Specialist TV Team",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/nouvellonsteph/specialist-tv.git"
	},
	"bugs": {
		"url": "https://github.com/nouvellonsteph/specialist-tv/issues"
	},
	"homepage": "https://github.com/nouvellonsteph/specialist-tv#readme",
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "rm -rf .next .open-next && next build",
		"start": "next start",
		"lint": "next lint",
		"deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
		"preview": "rm -rf .next .open-next && opennextjs-cloudflare build && opennextjs-cloudflare preview",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts"
	},
	"dependencies": {
		"@auth/core": "^0.40.0",
		"@auth/d1-adapter": "^1.10.0",
		"@cloudflare/puppeteer": "^1.0.4",
		"@cloudflare/stream-react": "^1.9.3",
		"@opennextjs/cloudflare": "^1.6.2",
		"next": "15.3.5",
		"next-auth": "^5.0.0-beta.29",
		"react": "^19.0.0",
		"react-dom": "^19.0.0"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3",
		"@tailwindcss/postcss": "^4",
		"@types/node": "^20.19.9",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"eslint": "^9",
		"eslint-config-next": "15.3.5",
		"tailwindcss": "^4",
		"typescript": "^5",
		"wrangler": "^4.27.0"
	}
}
